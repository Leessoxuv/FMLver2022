<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".UserInfo">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="961dp"
        android:layout_height="197dp"
        android:src="@drawable/logonv1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="412dp"
        android:layout_height="159dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="532dp"
        android:text="회원 정보"
        android:textColor="@color/black"
        android:textSize="45pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/userProfile"
        android:layout_width="283dp"
        android:layout_height="252dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/userprofile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/uploadImg"
        android:layout_width="286dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:text="사진 올리기"
        android:textColor="@color/black"
        android:textSize="15pt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userProfile" />

    <TextView
        android:id="@+id/Tv_id"
        android:layout_width="630dp"
        android:layout_height="242dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="3dp"
        android:text=" "
        android:textColor="@color/black"
        android:textSize="30pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/userProfile"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="197dp"
        android:layout_height="114dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="3dp"
        android:text="성별 : "
        android:textColor="@color/black"
        android:textSize="30pt"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/Et_Gender"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/Et_Gender"
        android:layout_width="627dp"
        android:layout_height="92dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="29dp"
        android:hint="성별을 입력하세요."
        android:textSize="25pt"
        app:layout_constraintBottom_toTopOf="@+id/Et_Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="200dp"
        android:layout_height="114dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="3dp"
        android:text="이름 : "
        android:textColor="@color/black"
        android:textSize="30pt"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toStartOf="@+id/Et_Name"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/Et_Name"
        android:layout_width="627dp"
        android:layout_height="92dp"
        android:layout_marginEnd="98dp"
        android:layout_marginBottom="15dp"
        android:hint="이름을 입력하세요."
        android:inputType="textPersonName"
        android:textSize="25pt"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="295dp"
        android:layout_height="97dp"
        android:layout_marginStart="11dp"
        android:layout_marginBottom="8dp"
        android:text="생년월일 : "
        android:textColor="@color/black"
        android:textSize="30pt"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/Et_Birth"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/Et_Birth"
        android:layout_width="627dp"
        android:layout_height="92dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="26dp"
        android:hint="생년월일을 입력하세요."
        android:inputType="date"
        android:textSize="25pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/Et_Name" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="295dp"
        android:layout_height="97dp"
        android:layout_marginStart="11dp"
        android:layout_marginBottom="560dp"
        android:text="전화번호 : "
        android:textColor="@color/black"
        android:textSize="30pt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Et_Number"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/Et_Number"
        android:layout_width="627dp"
        android:layout_height="92dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="26dp"
        android:hint="전화번호를 입력하세요."
        android:inputType="phone"
        android:textSize="25pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/Et_Birth" />

    <Button
        android:id="@+id/Btn_Save"
        android:layout_width="230dp"
        android:layout_height="81dp"
        android:layout_marginStart="704dp"
        android:layout_marginEnd="26dp"
        android:backgroundTint="#eeddff"
        android:text="저장"
        android:textColor="@color/black"
        android:textSize="20pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Et_Number" />


</androidx.constraintlayout.widget.ConstraintLayout>